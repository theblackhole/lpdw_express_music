<% include partials/header %>

<table border="1">
    <thead>
    <tr>
        <th>Title</th>
        <th>Album</th>
        <th>Artist</th>
        <th>Year</th>
        <th>BPM</th>
    </tr>
    </thead>
    <tbody>
    <% songs.forEach(function(song) {%>
        <tr>
            <td>
                <a href="/songs/<%=song._id%>"><%=song.title%></a>
            </td>
            <td><%=song.album%></td>
            <td><%=song.artist%></td>
            <td>
                <% if (song.year) { %><%=song.year%><% } %>
            </td>
            <td>
                <% if (song.bpm) { %><%=song.bpm%><% } %>
            </td>
        </tr>
    <% }) %>
    </tbody>
</table>
<% include partials/footer %>